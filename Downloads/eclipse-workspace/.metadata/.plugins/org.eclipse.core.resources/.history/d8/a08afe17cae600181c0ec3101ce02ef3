
public class SumTwoTrees {
    public TreeNode add(TreeNode a, TreeNode b) {
        if (a == null && b == null) {
        	return null;
        }
        else if (a == null && b != null) {
        	return new TreeNode(b.info, add(b.left), add(b.right));
        }
        else if (a != null && b == null) {
        	return new TreeNode(a.info, add(a.left), add(a.right));
        }
        return new TreeNode(a.info + b.info,
    }
}
